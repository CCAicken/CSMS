<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<package name="default" namespace="/" extends="struts-default">
		<action name="getcollege"
			class="action.control.getCollegeAction">
		</action>
		<action name="getmajor" class="action.control.getMajorAction"></action>
		<action name="getclass" class="action.control.getClassAction"></action>
		<action name="getforum" class="action.control.getForumAction"></action>
		<action name="getnew" class="action.control.getNewAction"></action>
		<action name="getcontent"
			class="action.control.getContentAction">
			<result name="success" type="dispatcher">
				/forumContent.jsp
			</result>
		</action>
		<action name="getproject"
			class="action.control.getProjectAction">
		</action>
		 
		
		<action name="getscore" class="action.control.getScoreAction">
			<result name="success" type="dispatcher">
				/scoreDetail.jsp
			</result>
		</action>
		<action name="login" class="action.control.LoginAction">

			<result name="success" type="dispatcher">/main.jsp</result>
			<result name="error" type="dispatcher">
				<param name="location">/error.jsp</param>
				<param name="errorsText">%{errorsText}</param>
				<param name="backurl">%{backurl}</param>
			</result>
		</action>
		<action name="logout" class="action.control.getLogoutAction"></action>
		<action name="getmacth" class="action.control.getMacthAction"></action>
		<action name="changepwd"
			class="action.control.ChangePwdAction">
		</action>
		<action name="getapplication"
			class="action.control.getApplication">
		</action></package></struts>    
